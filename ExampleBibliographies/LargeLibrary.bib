	keywords = {Brazil, Covid-19, NPIs, Physical distancing},
	pages = {3393--3400},
	file = {Full Text PDF:/Users/stephaniekestelman/Zotero/storage/T4I2HWPT/Moraes - 2020 - Determinants of physical distancing during the cov.pdf:application/pdf}
