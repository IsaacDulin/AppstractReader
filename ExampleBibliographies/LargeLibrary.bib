	doi = {10.2139/ssrn.3163930},
	keywords = {Abhijit V. Banerjee, Arun G. Chandrasekhar, Benjamin Golub, Emily Breza, SSRN, When Less is More: Experimental Evidence on Information Delivery During India's Demonetization}
}

