@techreport{blumenstock_impact_2020,
	title = {The {Impact} of {Mobile} {Phones}: {Experimental} {Evidence} from the {Random} {Assignment} of {New} {Cell} {Towers}},
	abstract = {We present experimental evidence on the economic impacts of mobile phone access. Our results are based on a randomized control trial in the Philippines, through which 14 isolated and previously unconnected villages were randomly assigned to either receive or not receive a new mobile phone tower. Following a pre-analysis plan, we ﬁnd that the introduction of mobile phones had large and signiﬁcant impacts on household income and expenditure, particularly for wage workers. Mobile phone access also increased social connections within and between communities. However, there are no consistent impacts on market access, informedness, or subjective well being. In analysis not prespeciﬁed, we ﬁnd suggestive evidence that the improved economic conditions are driven by increases in migration, remittances, and self-employment.},
	language = {en},
	author = {Blumenstock, Joshua and Keleher, Niall and Rezaee, Arman and Troland, Erin},
	year = {2020},
	pages = {50},
	file = {Blumenstock et al. - The Impact of Mobile Phones Experimental Evidence.pdf:/Users/stephaniekestelman/Zotero/storage/SQ5C8V9Q/Blumenstock et al. - The Impact of Mobile Phones Experimental Evidence.pdf:application/pdf;Blumenstock et al. - The Impact of Mobile Phones Experimental Evidence.pdf:/Users/stephaniekestelman/Zotero/storage/ZIC46XHM/Blumenstock et al. - The Impact of Mobile Phones Experimental Evidence.pdf:application/pdf;Blumenstock et al. - The Impact of Mobile Phones Experimental Evidence.pdf:/Users/stephaniekestelman/Zotero/storage/DYPDMTGA/Blumenstock et al. - The Impact of Mobile Phones Experimental Evidence.pdf:application/pdf}
