	author = {Chernozhukov, Victor and Hansen, Christian},
	year = {2005},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1468-0262.2005.00570.x},
	keywords = {identification, partial identification, bounded completeness, Endogeneity, instrumental regression, monotone likelihood ratio, nonlinear model, quantile regression, simultaneous equations},
	pages = {245--261},
	file = {Full Text PDF:/Users/stephaniekestelman/Zotero/storage/9NSTPZLJ/Chernozhukov and Hansen - 2005 - An IV Model of Quantile Treatment Effects.pdf:application/pdf;Snapshot:/Users/stephaniekestelman/Zotero/storage/33Z58JER/j.1468-0262.2005.00570.html:text/html}
}

