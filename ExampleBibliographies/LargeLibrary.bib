	keywords = {spatial},
	pages = {w27821},
	file = {Redding - 2020 - Trade and Geography.pdf:/Users/stephaniekestelman/Zotero/storage/AYU92QSE/Redding - 2020 - Trade and Geography.pdf:application/pdf}
}

