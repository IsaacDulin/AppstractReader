	month = jan,
	year = {2013},
	doi = {10.1016/B978-0-444-53759-1.00007-8},
	keywords = {public},
	pages = {391--474},
	file = {ScienceDirect Full Text PDF:/Users/stephaniekestelman/Zotero/storage/Z8U9R6UD/Piketty and Saez - 2013 - Chapter 7 - Optimal Labor Income Taxation.pdf:application/pdf}
}

@article{piketty_theory_2013,
	title = {A {Theory} of {Optimal} {Inheritance} {Taxation}},
	volume = {81},
	copyright = {© 2013 The Econometric Society},
	issn = {1468-0262},
	url = {http://onlinelibrary.wiley.com/doi/abs/10.3982/ECTA10712},
	doi = {https://doi.org/10.3982/ECTA10712},
	abstract = {This paper derives optimal inheritance tax formulas that capture the key equity-efficiency trade-off, are expressed in terms of estimable sufficient statistics, and are robust to the underlying structure of preferences. We consider dynamic stochastic models with general and heterogeneous bequest tastes and labor productivities. We limit ourselves to simple but realistic linear or two-bracket tax structures to obtain tractable formulas. We show that long-run optimal inheritance tax rates can always be expressed in terms of aggregate earnings and bequest elasticities with respect to tax rates, distributional parameters, and social preferences for redistribution. Those results carry over with tractable modifications to (a) the case with social discounting (instead of steady-state welfare maximization), (b) the case with partly accidental bequests, (c) the standard Barro–Becker dynastic model. The optimal tax rate is positive and quantitatively large if the elasticity of bequests to the tax rate is low, bequest concentration is high, and society cares mostly about those receiving little inheritance. We propose a calibration using micro-data for France and the United States. We find that, for realistic parameters, the optimal inheritance tax rate might be as large as 50\%–60\%—or even higher for top bequests, in line with historical experience.},
	language = {en},
	number = {5},
	urldate = {2021-01-28},
	journal = {Econometrica},
	author = {Piketty, Thomas and Saez, Emmanuel},
	year = {2013},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.3982/ECTA10712},
	keywords = {public},
	pages = {1851--1886},
	file = {Full Text PDF:/Users/stephaniekestelman/Zotero/storage/UIW6ZAYA/Piketty and Saez - 2013 - A Theory of Optimal Inheritance Taxation.pdf:application/pdf;Snapshot:/Users/stephaniekestelman/Zotero/storage/ZQ95BLCP/ECTA10712.html:text/html}
}

