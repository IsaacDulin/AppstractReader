	keywords = {econometrics},
	file = {Borusyak and Jaravel - Revisiting Event Study Designs,.pdf:/Users/stephaniekestelman/Zotero/storage/HCTX2WDH/Borusyak and Jaravel - Revisiting Event Study Designs,.pdf:application/pdf;Snapshot:/Users/stephaniekestelman/Zotero/storage/5BV4KA24/papers.html:text/html}
}

