@article{bitler_state_2010,
	title = {The {State} of the {Social} {Safety} {Net} in the {Post}-{Welfare} {Reform} {Era}},
	volume = {2010},
	issn = {1533-4465},
	url = {http://muse.jhu.edu/content/crossref/journals/brookings_papers_on_economic_activity/v2010/2010.2.bitler.html},
	doi = {10.1353/eca.2010.0013},
	abstract = {The 1996 welfare reform led to sweeping changes to the central cash safety net program for families with children. Along with other changes, the reform imposed lifetime time limits for receipt of cash welfare, effectively ending its entitlement nature for these families. Despite dire predictions, previous research has shown that program caseloads declined and employment increased, with no detectible increase in poverty or worsening of child well-being. We reevaluate these results in light of the severe 2007–09 recession. In particular, we examine how welfare reform has altered the cyclicality of the response of caseloads and family well-being. We ﬁnd that use of food stamps and noncash safety net program participation have become significantly more responsive to the economic cycle after welfare reform, rising more when unemployment increases. By contrast, we ﬁnd no evidence that cash welfare for families with children is more responsive, and some evidence that it might be less so. We ﬁnd some evidence that poverty increases more with increases in the unemployment rate after reform, and none that it increases less. We ﬁnd no signiﬁcant effects of reform on the cyclical responsiveness of food consumption, food insecurity, health insurance, household crowding, or health.},
	language = {en},
	number = {2},
	urldate = {2021-01-28},
	journal = {Brookings Papers on Economic Activity},
	author = {Bitler, Marianne P. and Hoynes, Hilary W.},
	year = {2010},
	keywords = {public, insurance},
	pages = {71--127},
	file = {Bitler and Hoynes - 2010 - The State of the Social Safety Net in the Post-Wel.pdf:/Users/stephaniekestelman/Zotero/storage/GFGF6SBG/Bitler and Hoynes - 2010 - The State of the Social Safety Net in the Post-Wel.pdf:application/pdf}
}

