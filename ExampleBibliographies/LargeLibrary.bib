	keywords = {public, labor},
	pages = {w17989},
	file = {Piketty and Saez - 2012 - A Theory of Optimal Capital Taxation.pdf:/Users/stephaniekestelman/Zotero/storage/4ZD4QEI7/Piketty and Saez - 2012 - A Theory of Optimal Capital Taxation.pdf:application/pdf}
}

